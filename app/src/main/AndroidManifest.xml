<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dubo.my_begin_test"
    android:versionCode="1"
    xmlns:tools="http://schemas.android.com/tools"
    android:versionName="1.0">

    <!--SD-->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        tools:replace="android:icon, android:theme,allowBackup"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/notitle">
        <activity android:name="com.example.dubo.myActivity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.dubo.myActivity.HomeActivity" android:screenOrientation="portrait"></activity>
        <activity android:name="com.example.dubo.myActivity.ImageActvity" android:screenOrientation="portrait"></activity>
        <activity android:name="imageActvity.ImgSelActivity" android:screenOrientation="portrait"></activity>
    </application>

</manifest>